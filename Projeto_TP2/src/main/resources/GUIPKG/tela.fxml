<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21.0.7" fx:controller="GUIPKG.KWICController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Pane prefHeight="600.0" prefWidth="900.0">
        <children>
            <Button fx:id="loadFileButton" layoutX="35.0" layoutY="529.0" mnemonicParsing="false" onAction="#onLoadFileButtonClick" text="Carregar" />
            <TextField fx:id="filePathField" layoutX="151.0" layoutY="527.0" prefHeight="25.0" prefWidth="301.0" />
            <Label fx:id="textFile" layoutX="42.0" layoutY="489.0" text="Arquivo Selecionado: " />
            <TextArea fx:id="inputTextArea" layoutX="30.0" layoutY="60.0" prefHeight="323.0" prefWidth="335.0" />
        </children>
    </Pane>
</VBox>